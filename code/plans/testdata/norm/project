ddl
(create-table abc
  [[a int] [b int] [c int]])
----
ok

ddl
(create-table xyz
  [[x int] [y int] [z int]])
----
ok

#plan
#(project abc [a b c])
#----
#-> Scan [1 2 3]
#
#plan
#(project
#  (project abc [b c])
#  [c])
#----
#-> Project [] [3]
# -> Scan [1 2 3]
#
#plan
#(project
#  (project abc [(+ b c) c])
#  [(+ 1 c)])
#----
#-> Project [5] []
# -> Scan [1 2 3]
# -> Plus
#  -> Constant (1)
#  -> ColRef (3)
#
#plan
#(project
#  (as (project abc [(* b c) c])
#  _ [d e])
#  [d])
#----
#-> Project [4] []
# -> Scan [1 2 3]
# -> Times
#  -> ColRef (2)
#  -> ColRef (3)

plan
(as (project
  abc
  [(+ a a)]) _ [a])
----
-> Project [4] []
 -> Scan [1 2 3]
 -> Times
  -> Constant (2)
  -> ColRef (1)

plan
(as (project
(as (project
(as (project
(as (project
(as (project
  abc
  [(+ a a)]) _ [a])
  [(+ a a)]) _ [a])
  [(+ a a)]) _ [a])
  [(+ a a)]) _ [a])
  [(+ a a)]) _ [a])
----
-> Project [8] []
 -> Scan [1 2 3]
 -> Times
  -> Constant (2)
  -> Times
   -> Constant (2)
   -> Times
    -> Constant (2)
    -> Times
     -> Constant (2)
     -> Times
      -> Constant (2)
      -> ColRef (1)
